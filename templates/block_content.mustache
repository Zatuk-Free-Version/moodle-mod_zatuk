{{!
    @template mod_zatuk/block_content
    Example context (json):
    {
        "uploadedVideos": "10",
        "syncedVideos": "5",
        "totalVideos": "5",
        "viewcap": "true"
    }
}}
<div class="zatuk_block">
	<ul>
		<li>{{#str}} uploadedvideos, mod_zatuk {{/str}} {{uploadedVideos}}</li>
		<li>{{#str}} syncvideos, mod_zatuk {{/str}} {{syncedVideos}}</li>
		<li>{{#str}} totalvideos, mod_zatuk {{/str}} {{totalVideos}}</li>
	</ul>
	{{# viewcap}}
	<a href="{{config.wwwroot}}/mod/zatuk/index.php" class="pull-right">{{#str}} viewmore, mod_zatuk {{/str}} </a>
	{{/ viewcap}}
</div>
